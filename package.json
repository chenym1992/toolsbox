{
  "name": "fe-toolsbox",
  "version": "0.0.9",
  "packageManager": "pnpm@7.9.0",
  "description": "前端业务常用工具库",
  "main": "dist/toolsbox.common.js",
  "module": "dist/toolsbox.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "scripts": {
    "test": "jest",
    "format": "prettier --write --parser typescript \"(src|test|packages|types)/**/*.ts\"",
    "prepare": "husky install",
    "release": "node scripts/build.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --ignore-unknown"
    ],
    "*.ts": [
      "prettier --parser=typescript --write --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenym1992/toolsbox.git"
  },
  "keywords": [
    "typescript",
    "toolsbox",
    "jest",
    "rollup"
  ],
  "author": "chenym1992",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chenym1992/toolsbox/issues"
  },
  "homepage": "https://github.com/chenym1992/toolsbox#readme",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "conventional-changelog-cli": "^2.2.2",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.5",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4",
    "vitepress": "1.0.0-alpha.5",
    "vue": "^3.2.37"
  }
}
